programa {
  funcao inicio() {
    inteiro num1
    escreva("Digite um numero:")
    leia(num1)
  
   

    se(num1 > 0)
      escreva("Seu valor é positivo")
    senao se(num1 < 0)
      escreva("Seu numero é negativo")
    senao 
      escreva("Seu primeiro numero é igual a 0 \n")
     
    
  }
}

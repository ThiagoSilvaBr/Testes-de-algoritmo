 Algoritmo "semnome"




Var
      palavra, palavraDois, palavraTres: caractere
inicio
     escreva("Digite uma palavra: ")
     leia(palavra)
     escreva("Digite outra palavra: ")
     leia(palavraDois)
     palavraTres <- palavra+palavraDois
     escreval("a sua palavra é:", palavraTres)
fimalgoritmo